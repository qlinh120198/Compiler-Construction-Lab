program MAXELEMENT;
type a = array(.10.) of integer;
var arr : a;

function largest(max : integer) : integer; (* param type is basic type - char and integer only *)
var i : integer;
begin
    for i := 0 to 10 do
      begin
          if arr(.i.) > max then
          begin
              max := arr(.i.);
          end;
      end;
    largest := max; (* return statement in kpl *)
end;
